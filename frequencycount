<!DOCTYPE html>
<html>
<head>
  <title>Frequency Count</title>
</head>
<body>
  <h1>Frequency Count</h1>
  
  <script>
    function countFrequency(arr) {
      var frequency = {};
      
      for (var i = 0; i < arr.length; i++) {
        var element = arr[i];
        
        if (frequency[element]) {
          frequency[element]++;
        } else {
          frequency[element] = 1;
        }
      }
      
      return frequency;
    }
    
    var array = [1, 2, 3, 2, 1, 3, 1, 2, 3, 4, 5, 4, 4];
    var result = countFrequency(array);
    
    console.log(result);
  </script>
</body>
</html>
